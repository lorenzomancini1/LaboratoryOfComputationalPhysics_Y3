   "source": [
    "radii = [1,2,3]\n",
    "pi = 3.1415926\n",
    "Vol = [[(lambda x: 2*pi*x)(i), (lambda x: pi*x*x)(i), (lambda x: 3/4*pi*x*x*x)(i)] for i in radii]\n",
