   "source": [
    "def fnew(alist, x):\n",
    "    l = alist + [i for i in range(x)]\n",
    "    return l\n",
    "\n",
    "alist = [1,2,3]\n",
    "ans = fnew(alist, 5)\n",
    "print (ans)\n",
    "print (alist) # alist has NOT been changed!"
